{
  "manifest_version": 2,
  "name": "StockMon",
  "description": "A currently clone",
  "version": "0.1",
  "permissions": [
    "http://api.wunderground.com/api/",
    "http://autocomplete.wunderground.com/api"
  ],
  
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "/templates/tabs.html"
  }
}